<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/shape">
        <shape
            android:shape="rectangle" >

            <solid
                android:color="#FFFFFF">
            </solid>

            <stroke
                android:width="1dp"
                android:color="@color/tagBorder" >
            </stroke>

            <corners
                android:radius="20dp">
            </corners>

        </shape>
    </item>
    <item android:id="@+id/icon">
        <bitmap android:gravity="center" android:src="@drawable/cross10"/>
    </item>
</layer-list>